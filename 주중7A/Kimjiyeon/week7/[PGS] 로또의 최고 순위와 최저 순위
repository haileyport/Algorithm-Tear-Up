function solution(lottos, win_nums) {
    const correct = lottos.filter(lotto => win_nums.includes(lotto)).length;
    //맞은갯수
    const zeros = lottos.filter(lotto => lotto === 0).length;
    //0갯수    
    let min = 0;
	  if(correct <= 1){
		//낙첨일때
        min = 6
    }else{
        min = 7-correct
    }
    let max = 0;
    if(min-zeros < 1){
        max = 1
    }else{
        max = min-zeros
    }
return [max, min];
}
